<section>
    <h2>clothing List</h2>

    <a href="/admin/clothing/createClothing">create clothing</a>
    <ul>
        <% if (clothing.length > 0) { %>
            <% clothing.forEach((clothe) => { %>
                <div data-id="<%=clothe.clothing_id%>">
                    <li><%= clothe.clothing_id %></li>
                    <li><%= clothe.clothing_name %></li>
                    <a class="edit" href="/admin/clothing/editCLothing/<%=clothe.clothing_id%>">edit</a>
                    <button class="js-remove-clothe-button" data-id="<%=clothe.clothing_id%>">supp</button>
                </div>
            <% }); %>
        <% } else { %>
            <li>No clothing is aviable.</li>
        <% } %>
    </ul>
</section>
<script src="/js/index.js"></script>